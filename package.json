    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-ws": "9.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/websockets": "9.0.0",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/spl-token": "^0.4.12",
    "@solana/web3.js": "^1.98.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/lodash": "^4.17.5",
    "asn1.js": "^5.4.1",
    "axios": "^1.6.8",
    "bignumber.js": "^9.1.1",
    "buffer": "^6.0.3",
    "bull": "^4.16.5",
    "crypto": "^1.0.1",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2",
    "helmet": "^6.0.1",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nest-winston": "^1.10.0",
    "os": "^0.1.2",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
