{
  "name": "chain-observer",
  "version": "1.0.0",
  "description": "Observer chain contract event",
  "author": "joneywon666@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "set NODE_ENV=production&& nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "set NODE_ENV=development&& nest start",
    "start:dev": "set NODE_ENV=development&& nest start --watch",
    "start:debug": "set NODE_ENV=development&& nest start --debug --watch",
    "start:prod": "set NODE_ENV=production&& node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:build": "docker build -t bigdata .",
    "docker:run:local": "docker run -itd -v D:\\app\\safebox-db:/data chain-observer yarn start:prod"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
